    "version": "0.2.0",
    "configurations": [
        {
            "type": "OctaveDebugger",
            "request": "launch",
            "name": "Function or file to execute.",
            "program": "${command:AskForProgramName}",
            "octave": "octave-5.2.0",
            "sourceFolder": "${workspaceFolder}",
            "autoTerminate": true
        },

        {
            "type": "OctaveDebugger",
            "request": "launch",
            "name": "Launch ex7",
            "program": "ex7.m",
            "octave": "octave-5.2.0",
            "sourceFolder": "${workspaceFolder}",
            "autoTerminate": true
        },

        {
            "type": "OctaveDebugger",
            "request": "launch",
            "name": "Launch ex7_pca",
            "program": "ex7_pca.m",
            "octave": "octave-5.2.0",
            "sourceFolder": "${workspaceFolder}",
            "autoTerminate": true
        }
    ]
}